generator client {
  provider = "prisma-client-js"
  //output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id          Int   @id @default(autoincrement())
  name        String
  list   TasksList @relation(fields: [listId], references: [id])
  listId Int
}

model TasksList {
  id    Int    @id @default(autoincrement())
  name  String
  tasks Task[]
}